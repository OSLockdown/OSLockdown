<?xml version="1.0" encoding="UTF-8"?>
<security_module sortKey="100" name="Disable Printer Configuration Daemon">
        <library>DisablePrinterConfig</library>
        <description>Disables the CUPS printer configuration daemon that is used to configure the CUPS subsystem.</description>
        <scan_weight>1</scan_weight>
        <action_weight>1</action_weight>
        <severity_level>1</severity_level>
