<?xml version="1.0" encoding="UTF-8"?>
<security_module sortKey="100" name="Disable SETroubleshoot">
        <library>DisableSETroubleshoot</library>
        <description>Disabling the SETroubleshoot service will prevent desktop users from being notified of SELinux deials. On Red Hat systems, the SETroubleshoot service reports denials in a in a user-friendly fashion but SELinux errors may provide important information about intrusion attempts in progress, or may give information about SELinux configuration problems which are preventing correct system operation. </description>
        <scan_weight>1</scan_weight>
        <action_weight>1</action_weight>
        <severity_level>1</severity_level>
        <platforms>
          <cpe-item name="cpe:/o:centos:centos:5">
            <services>
              <service seq="1" name="setroubleshoot"/>
            </services>
            <packages>
              <package name="setroubleshoot"/>
            </packages>
          </cpe-item>
          <cpe-item name="cpe:/o:redhat:enterprise_linux:5">
            <services>
              <service seq="1" name="setroubleshoot"/>
            </services>
            <packages>
              <package name="setroubleshoot"/>
            </packages>
          </cpe-item>
          <cpe-item name="cpe:/o:oracle:enterprise_linux:5">
            <services>
              <service seq="1" name="setroubleshoot"/>
            </services>
            <packages>
              <package name="setroubleshoot"/>
            </packages>
          </cpe-item>
          <cpe-item name="cpe:/o:redhat:fedora:10">
            <services>
              <service seq="1" name="setroubleshoot"/>
            </services>
            <packages>
              <package name="setroubleshoot"/>
            </packages>
          </cpe-item>
          <cpe-item name="cpe:/o:redhat:fedora:11">
            <packages>
              <package name="setroubleshoot"/>
              <package name="setroubleshoot-server"/>
            </packages>
          </cpe-item>
          <cpe-item name="cpe:/o:redhat:fedora:12">
            <packages>
