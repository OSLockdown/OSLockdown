<?xml version="1.0" encoding="UTF-8"?>
<security_module sortKey="100" name="Use NOSUID and NODEV for Removable Media">
        <library>FstabRemovableMedia</library>
        <description>Places the nodev and nosuid options on removable media partitions, which prevents users from bringing set-UID programs into the system via CD-ROMs and floppy disks.</description>
        <scan_weight>1</scan_weight>
        <action_weight>1</action_weight>
        <severity_level>1</severity_level>
